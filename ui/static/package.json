{
  "name": "client",
  "version": "1.0.0",
  "description": "",
  "main": "bundle.js",
  "dependencies": {
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "bootstrap": "^3.3.7",
    "catw": "^1.0.1",
    "grunt-cli": "^1.2.0",
    "less": "^2.7.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-loading": "0.0.9",
    "rest": "^2.0.0",
    "watchify": "^3.7.0"
  },
  "devDependencies": {},
  "scripts": {
    "watch-css": "node_modules/.bin/catw -c 'lessc -' 'less/*.less' -o bundle.css -v",
    "watch-js": "watchify js/app.js -t [ babelify --presets [ es2015 react ] ] -o bundle.js -v",
    "start": "npm run watch-css & npm run watch-js",
    "build-css": "node_modules/.bin/catw -c 'lessc -' 'less/*.less' > bundle.css",
    "build-js": "browserify js/app.js -t [ babelify --presets [ es2015 react ] ] -o bundle.js -v",
    "build": "npm run build-css && npm run build-js",
    "test": "jest"
  },
